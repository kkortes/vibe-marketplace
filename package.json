{
  "name": "vibe-marketplace",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun compile:watch & bunx live-server compiled --port=3000 --host=localhost",
    "build": "bun compile",
    "compile": "bun vibe compile && mv compiled/node_modules compiled/modules",
    "compile:watch": "bun compile && bun vibe compile --watch --no-clean --fouc-as-is"
  },
  "dependencies": {
    "@ape-egg/vibe": "^1.7.2",
    "async-await-websockets": "^3.0.5"
  },
  "vibe-compiler": {
    "source": "./",
    "output": "./compiled",
    "components": "components",
    "elementsAsIs": true,
    "nodeModulesAsIs": false,
    "minify": false
  }
}
